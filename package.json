{
  "name": "generator-fractal-component",
  "version": "0.0.5",
  "description": "Generates fractal component boilerplate",
  "author": {
    "name": "superhuit",
    "url": "https://superhuit.ch"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.10.0",
    "eslint-config-xo": "^0.20.1",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^0.14.3",
    "jest": "^22.4.4",
    "lint-staged": "^6.1.1",
    "merge": ">=1.2.1",
    "nsp": "^2.8.0",
    "prettier": "^1.14.2",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.9.1"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "inquirer-directory": "^2.1.0",
    "inquirer-file-path": "^1.0.1",
    "inquirer-folder-explorer": "^2.0.0",
    "inquirer-path": "^1.0.0-beta5",
    "inquirer-select-directory": "^1.2.0",
    "yeoman-generator": "^2.0.5",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "license": "MIT"
}
